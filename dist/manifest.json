{
    "manifest_version": 3,
    "name": "Emoji Keyboard 1999",
    "version": "1.0",
    "description": "Integrate emojis into text fields.",
    "icons": {
      "128": "icon.png"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "128": "icon.png"
      }
    }
  }
  